<div class="form_new_review">
  <div class="container_2">
    <%= form_for @review do |f| %>
      <% f.label :user %>
      <input type="text" name="review[user]" placeholder="User Name"> <br />

      <% f.label :title %>
      <%= f.text_field :title%> <br />

      <% f.label :description %>
      <%= f.text_field :description%> <br />

      <% f.label :score %>
      <%= f.text_field :score%> <br />

      <%= f.submit %>
    <%end %>
  </div>
</div>
