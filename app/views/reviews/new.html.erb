<div class="form_new_review">
  <div class="container_2">
    <%= form_for [@book, @review] do |f| %>
      <% f.label :user %>
      <%= f.text_field :user, placeholder: "User Name" %> <br />

      <% f.label :title %>
      <%= f.text_field :title%> <br />

      <% f.label :description %>
      <%= f.text_area :description%> <br />

      <% f.label :score %>
      <%= f.text_field :score%> <br />

      <%= f.submit %>
    <%end %>
  </div>
</div>
